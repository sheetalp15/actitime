<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite guice-stage="DEVELOPMENT" name="Selenium_training by groups">
  <test thread-count="5" verbose="2" name="GRP-cars">
    <groups>
      <run>
        <include name="cars"/>      //if you exclude tag, the cars that are part of cars group will be excluded
        <exclude name="maruthi"/> 
      </run>
    </groups>
    <classes>
      <class name="com.selenium.basic.TestNG.GroupsDemo"/>
    </classes>
  </test> <!-- GRP-cars -->
</suite> <!-- Selenium_training by groups -->
            